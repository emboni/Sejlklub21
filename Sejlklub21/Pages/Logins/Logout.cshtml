@page
@model Sejlklub21.Pages.Accounts.LogoutModel
@{
    ViewData["Title"] = "Logout";
}

<h1>Logout</h1>

<form method="post">
    <div class="form-group">
        <label class="control-label">@Model.CurrentMember.Name</label>
    </div>
    <div class="form-group">
        <input type="submit" value="Logout" class="btn btn-danger" />
        <input type="button" value="Back" class="btn btn-link" />
    </div>
</form>